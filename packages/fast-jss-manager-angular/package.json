{
  "name": "@microsoft/fast-angular-jss-manager",
  "description": "Angular component JSS stylesheets",
  "author": {
    "name": "Fluent Web",
    "email": "fwq@microsoft.com"
  },
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch && npm-watch",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "lint:fix": "tslint -c tslint.json ./src/**/*.ts --fix",
    "lint:watch": "node build/tslint-watch.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "keywords": [
    "angular",
    "jss"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues"
  },
  "homepage": "https://github.com/Microsoft/fast-dna#readme",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@microsoft/fast-tslint-rules": "^1.0.4",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "jest": "^22.4.2",
    "rxjs": "^5.5.7",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/core": "^5.2.9",
    "rxjs": "^5.5.7"
  },
  "dependencies": {
    "@microsoft/fast-jss-manager-react": "^1.0.0",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "uuid": "^3.2.1"
  }
}
